{
  "name": "FBOL-CULTIVARTE-BACK-MNGR",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && tsc && cpy 'src/assets/**/*' 'dist/assets/' --parents && tsc-alias",
    "start": "node dist/main.js",
    "start:dev": "nodemon src/main.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./e2e/jest.json",
    "prepare": "if [ \"$NODE_ENV\" = \"local\" ]; then husky install; fi"
  },
  "keywords": [
    "node",
    "mngr",
    "manager",
    "adapter",
    "express",
    "davivienda"
  ],
  "author": "Brayan Sanjuan <brayan.sanjuan@davivienda.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@google-cloud/secret-manager": "^6.1.1",
    "@graphql-tools/merge": "^9.1.0",
    "@npm-davi/davi-coe-tslog-nodejs-lib": "~1.0.0",
    "@types/jest": "^30.0.0",
    "body-parser": "~2.2.0",
    "cors": "^2.8.5",
    "dotenv": "~16.4.7",
    "express": "^4.21.2",
    "express-openapi-validator": "~5.4.9",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "helmet": "~8.1.0",
    "module-alias": "~2.2.3",
    "morgan": "~1.10.0",
    "pg": "^8.11.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "uuid": "~11.1.0",
    "zod": "~3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "~19.8.0",
    "@commitlint/config-conventional": "~19.8.0",
    "@eslint/eslintrc": "~3.2.0",
    "@eslint/js": "~9.24.0",
    "@types/cors": "^2.8.18",
    "@types/jest": "~29.5.14",
    "@types/morgan": "~1.9.9",
    "@types/node": "~22.14.0",
    "@types/pg": "^8.15.1",
    "@types/supertest": "~6.0.3",
    "@typescript-eslint/eslint-plugin": "~8.29.1",
    "@typescript-eslint/parser": "~8.29.1",
    "cpy-cli": "~5.0.0",
    "eslint": "~9.33.0",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-prettier": "~5.5.4",
    "globals": "~16.0.0",
    "husky": "~9.1.7",
    "jest": "~29.7.0",
    "node-mocks-http": "~1.16.2",
    "nodemon": "~3.1.9",
    "prettier": "~3.6.2",
    "rimraf": "^6.1.0",
    "source-map-support": "~0.5.21",
    "supertest": "~7.1.0",
    "ts-jest": "~29.3.1",
    "ts-loader": "~9.5.2",
    "ts-node": "~10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "~1.8.14",
    "tsconfig-paths": "~4.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.29.1"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.route.ts",
      "!**/*.d.(t|j)s",
      "!**/main.(t|j)s",
      "!**/index.(t|j)s",
      "!**/logger.(t|j)s",
      "!**/*.middleware.ts",
      "!**/configuration.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    }
  }
}
